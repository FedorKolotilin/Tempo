<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fon">

    <FrameLayout
        android:id="@+id/imageView2"
        android:layout_width="363dp"
        android:layout_height="248dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="208dp"
        android:layout_marginEnd="31dp"
        android:foreground="@drawable/fon_ocno_vbor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fon_ocno_vbor" />


    <EditText
        android:id="@+id/room_entering_id"
        android:layout_width="271dp"
        android:layout_height="45dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="45dp"
        android:backgroundTint="@color/white_2"
        android:backgroundTintMode="add"
        android:ems="10"
        android:fontFamily="@font/manrope_bold"
        android:foregroundTintMode="add"
        android:text="@string/ID"
        android:textColor="@color/dark_corch"
        android:textSize="18sp"
        app:iconTint="@color/white"
        app:iconTintMode="add"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        tools:text="@string/ID" />

    <EditText
        android:id="@+id/room_entering_password"
        android:layout_width="268dp"
        android:layout_height="46dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="46dp"
        android:backgroundTint="@color/white_2"
        android:backgroundTintMode="add"
        android:ems="10"
        android:fontFamily="@font/manrope_bold"
        android:foregroundTintMode="add"
        android:text="@string/Password"
        android:textColor="@color/dark_corch"
        android:textSize="18sp"
        app:iconTint="@color/white"
        app:iconTintMode="add"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/room_entering_id"
        tools:text="@string/Password" />

    <ImageButton
        android:id="@+id/room_entering_enter"
        android:layout_width="329dp"
        android:layout_height="95dp"
        android:layout_marginEnd="14dp"
        android:background="@drawable/join"
        android:foreground="@drawable/join"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2" />
</androidx.constraintlayout.widget.ConstraintLayout>